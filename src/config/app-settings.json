{
   "application": {
      "environment": "DEV",
      "port":1337,
      "storage": {
         "user": "../../public/images/users/",
         "category": "../../public/images/categories/",
         "product": "../../public/images/products/"
      },
      "getImagePath":{
         "product":"http://localhost:1337/images/products/"
      }
   },
   "database": {
      "type": "mysql",
      "host": "localhost",
      "port": 3306,
      "username": "user",
      "password": "root",
      "database": "ecommerce",
      "queueLimit": 0,
      "waitForConnections": true,
      "decimalNumbers": true,
      "namedPlaceholders": false,
      "synchronize": true,
      "logging": false,
      "sslCertifciate": "../ssl/BaltimoreCyberTrustRoot.crt.pem",
      "entities": [
         "src/entity/**/*.ts",
         "entity/**/*.js"
      ],
      "migrations": [
         "src/migration/**/*.ts"
      ],
      "subscribers": [
         "src/subscriber/**/*.ts"
      ],
      "cli": {
         "entitiesDir": "src/entity",
         "migrationsDir": "src/migration",
         "subscribersDir": "src/subscriber"
      }
   }
}